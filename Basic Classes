import java.util.*;
public class dev {
    public static void main(String args[]){
        Pen p1 = new Pen();
        //Scanner sc = new Scanner(System.in);

        p1.setcolor();
        
    }

}

class Pen{

    String color;
    int size;
    Scanner sc = new Scanner(System.in);

    void setcolor(){    //String nc1, String nc2
        String nc1;
        String nc2;

        nc1 = sc.nextLine();
        nc2 = sc.next();
        
        /*
        nextLine()      : all the words are in a single string
        next()          : will split the line up into individual words, returning individual text Strings one at a time.
        */
        
        color = nc1;
        System.out.println(nc2);
        System.out.println(color);
    }

    void setsize(int newsize){
        size = newsize;
    }
}

class Student{
    int p,c,m,avg;
}
